	booktitle = "{{DC-2011}, The Hague}",
	copyright = "The copyright for submission is retained by the author(s), with first publication rights granted to {DCMI} for publication in the electronic and print proceedings. By virtue of their appearance in this open access publication, submissions are free to be used with proper attribution for educational and other non-commercial purposes. Other uses may require the permission of the author(s).",
	file = "Haslhofer\_Isaac\_2011\_data.pdf:C:\Dokumente und Einstellungen\mittelb\Anwendungsdaten\Mozilla\Firefox\bibo\zotero\storage\VXSRFPKS\Haslhofer\_Isaac\_2011\_data.pdf:application/pdf;Snapshot:C:\Dokumente und Einstellungen\mittelb\Anwendungsdaten\Mozilla\Firefox\bibo\zotero\storage\IAV6QJH3\55.html:text/html",
